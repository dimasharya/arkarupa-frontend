{
  "name": "the-arkarupa-apps",
  "version": "1.0.0",
  "description": "Project Management",
  "private": true,
  "scripts": {
    "tailwind:dev": "tailwindcss build src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "tailwind:build": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "prestart": "npm run tailwind:dev",
    "start": "react-scripts --openssl-legacy-provider start",
    "prebuild": "npm run tailwind:build",
    "build": "react-scripts --openssl-legacy-provider build",
    "cz": "git-cz",
    "release": "release-it",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@dhtmlx/gantt-data-provider": "^0.2.0",
    "@dhtmlx/trial-react-gantt": "^1.1.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@heroicons/react": "^1.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "@themesberg/tailwind-datepicker": "^1.1.0",
    "@windmill/react-ui": "^0.6.0",
    "@wojtekmaj/react-daterange-picker": "^3.4.0",
    "chart.js": "^3.5.1",
    "chokidar": "^3.5.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "faker": "^5.5.3",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.5",
    "react-dom": "^17.0.2",
    "react-dropzone-uploader": "^2.11.0",
    "react-focus-lock": "^2.5.2",
    "react-hook-form": "^7.20.2",
    "react-hot-toast": "^2.1.1",
    "react-icons": "^4.3.1",
    "react-moment": "^1.1.1",
    "react-number-format": "^4.7.3",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-step-wizard": "^5.3.9",
    "react-table": "^7.8.0",
    "react-tooltip": "^4.2.21",
    "react-transition-group": "^4.4.2",
    "tailwind-scrollbar-hide": "^1.0.3",
    "uniqid": "^5.4.0",
    "windmill-react-ui-kit": "^0.7.4"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^3.3.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "cz-conventional-changelog": "^3.3.0",
    "react-axe": "^3.5.4",
    "react-error-overlay": "^6.0.9",
    "release-it": "^14.11.6",
    "tailwindcss": "^2.2.19",
    "yarn-upgrade-all": "^0.5.4"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
